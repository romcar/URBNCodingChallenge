{
  "name": "urbncodingchallenge",
  "version": "1.0.0",
  "description": "An application using the Eventful API to locate events happening near you.",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --watch",
    "start": "webpack-dev-server --config webpack.config.dev.js",
    "server": "nodemon --inspect server/serverConnection.js",
    "lint": "eslint ."
  },
  "jest": {
    "verbose": false,
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "<rootDir>/src/assets/CSSStub.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/src/assets/setupTest.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romcar/URBNCodingChallenge.git"
  },
  "keywords": [
    "eventful",
    "events"
  ],
  "author": "Erwin Carrasquilla <rommel.carrasquilla@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/romcar/URBNCodingChallenge/issues"
  },
  "homepage": "https://github.com/romcar/URBNCodingChallenge#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-jest": "^23.4.2",
    "memory-cache": "^0.2.0",
    "moment": "^2.22.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-responsive-modal": "^3.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.3",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "enzyme-to-json": "^3.3.4",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "react-hot-loader": "^4.3.5",
    "request": "^2.88.0",
    "style-loader": "^0.23.0",
    "styled-components": "^3.4.5",
    "styled-components-grid": "^2.2.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-dev-server": "^3.1.6",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-merge": "^4.1.4"
  }
}
